<script>
  jQuery(function ($) { // Using https://github.com/sdepold/jquery-rss
    $("#rss-feed").rss("{{{feed}}}",
      {
        layoutTemplate: "<div class='default-ticker'> {entries} </div>",
        entryTemplate: "<div class='feed-text'> {title} <span class='bullet'>&#8226;</span></div>",
        host: 'feedrapp.info',
        limit: 10
      })
  })
</script>

<div id="wrapper">
  <div id="rss-feed"></div>
  <div id="bar"></div>
</div>

